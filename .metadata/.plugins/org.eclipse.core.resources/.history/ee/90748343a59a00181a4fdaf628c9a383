spring.application.name=server02jdbc
########################################################
#configuracion 
#cambiamos el puerto donde e va a ejecutar la aplicacion
server.port=8020

#ponemos la url de acceso a la base de datos
spring.datasource.url=jdbc:postgresql://localhost:5432/db_distribuida
#usuario
spring.datasource.username=postgres
#contraseña
spring.datasource.password=root
#que tipo de base de datos es
spring.datasource.platform=postgresql





###############################################
#configuracion a eureka
#server.port=8761
eureka.client.register-with-eureka=true
eureka.client.serviceUrl.defaultZone=http://localhost:8761/eureka/
eureka.client.fetch-registry=true
